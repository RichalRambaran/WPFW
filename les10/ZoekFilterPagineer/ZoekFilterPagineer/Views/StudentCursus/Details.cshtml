@model ZoekFilterPagineer.Models.StudentCursus

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>StudentCursus</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Student)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Student.StudentNaam)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Cursus)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Cursus.CursusNaam)
        </dd>
    </dl>
</div>
<div>
    @Html.ActionLink("Edit", "Edit", new { StudentId = Model.Student.StudentId, CursusId = Model.Cursus.CursusId }) |
